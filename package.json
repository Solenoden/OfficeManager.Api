{
  "name": "officemanager.api",
  "version": "1.0.0",
  "description": "The API for the Office Manager Web application which allows you to capture offices and the people who work in them",
  "main": "dist/app.js",
  "scripts": {
    "start": "node .",
    "build": "tsc",
    "prestart": "npm run build",
    "lint": "eslint ./",
    "start-containerized-environment": "docker-compose -f ./containerized-environment/docker-compose.yaml -p \"office-manager-environment\" up --detach"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Solenoden/OfficeManager.Api.git"
  },
  "author": "Gavin de Bruyn <me@gavindebruyn.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Solenoden/OfficeManager.Api/issues"
  },
  "homepage": "https://github.com/Solenoden/OfficeManager.Api#readme",
  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "morgan": "1.10.0",
    "pg": "8.8.0",
    "typescript": "4.4.4"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.1",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.4",
    "@types/pg": "8.6.5",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "eslint": "7.30.0"
  }
}
